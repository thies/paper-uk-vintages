file = {:home/thies/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2000 - Over-Confidence and Cycles in Real Estate Markets Cases in Hong Kong and Asia.pdf:pdf},
journal = {International Real Estate Review},
