issn = {08955638},
journal = {Journal of Real Estate Finance and Economics},
keywords = {House prices,Kalman filter,Signal-extraction,Smoothing,State-space models},
number = {1},
pages = {24--52},
title = {{Repeat sales index for thin markets}},
volume = {41},
year = {2010}
}
