doi = {10.1016/j.regsciurbeco.2008.06.002},
file = {:home/thies/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bostic, Gabriel, Painter - 2009 - Housing wealth, financial wealth, and consumption New evidence from micro data.pdf:pdf},
isbn = {0166-0462},
issn = {01660462},
journal = {Regional Science and Urban Economics},
keywords = {Aggregate output,Consumption,Elasticity,Housing,Wealth},
number = {1},
