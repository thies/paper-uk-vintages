@article{Feng2007,
abstract = {Much of the literature on capital structure excludes Real Estate Investment Trusts (REITs) due mainly to the unique regulatory environment of these firms. As such, the issue of how REITs choose among different financing options when they raise external capital is largely unexplored. In this paper, we explore two issues on the capital structure of REITs: is there a relationship between market-to-book and leverage ratios, and, is the relationship between market-to-book and leverage ratio temporary or persistent. Our results suggest that REITs with historically high marketto- book ratio tend to have persistently high leverage ratio. In essence, REITs with high growth opportunity and high market valuation raise funds through debt issues. This finding, which is robust to various specifications and econometric tests, is contrary to the financing decisions of non-regulated firms. We attribute it to the special regulatory environment of REITs where, despite no apparent benefits to debt financing, management issues debt.},
author = {Feng, Zhilan and Ghosh, Chinmoy and Sirmans, C. F.},
doi = {10.1007/s11146-007-9005-2},
