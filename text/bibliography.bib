issn = {01973975},
journal = {Habitat International},
keywords = {Housing prices,Housing supply,Land-use planning system,Planning applications},
number = {3},
pages = {339--359},
title = {{Does the planning system affect housing prices? Theory and with evidence from Hong Kong}},
volume = {27},
year = {2003}
}
