journal = {Regional Science and Urban Economics},
keywords = {Spatial Durbin model,Spatial econometrics,Spatial lag model,Spatial weight matrix,Trans-dimensional simulated annealing},
number = {3},
pages = {429--444},
publisher = {Elsevier B.V.},
title = {{Automatic selection of a spatial weight matrix in spatial econometrics: Application to a spatial hedonic approach}},
