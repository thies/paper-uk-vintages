journal = {Journal of Economic Behavior and Organization},
keywords = {Anchoring,Behavioral economics,Housing,Listing prices},
pages = {76--92},
publisher = {Elsevier B.V.},
title = {{A homeowner's dilemma: Anchoring in residential real estate transactions}},
url = {http://dx.doi.org/10.1016/j.jebo.2013.01.010},
volume = {89},
year = {2013}
}
