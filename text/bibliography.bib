pages = {425--434},
title = {{Detection of individual trees and estimation of tree height using LiDAR data}},
url = {http://dx.doi.org/10.1007/s10310-007-0041-9%5Cnhttp://www.springerlink.com.offcampus.lib.washington.edu/index/2835L5602651P467.pdf},
